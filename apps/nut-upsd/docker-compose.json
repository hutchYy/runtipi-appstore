{
  "services": [
    {
      "name": "NUT UPS",
      "image": "instantlinux/nut-upsd:latest",
      "isMain": true,
      "internalPort": 3493,
      "environment": {
        "TZ": "${TZ:-Europe/Brussels}",
        "API_PASSWORD": "${API_PASSWORD}",
        "DRIVER": "usbhid-ups"
      },
      "devices": [
        "/dev/bus/usb:/dev/bus/usb"
      ]
    }
  ],
  "$schema": "../dynamic-compose-schema.json"
}